@model WebOblig1.Model.DomeneKonto

@using (Html.BeginForm("Transaksjoner", "Admin")) {

<div class="form-group col-md-4">
        @Html.LabelFor(model => model.kontonummer)
        @Html.EditorFor(model => model.kontonummer, new { htmlAttributes = new {
            @class = "form-control",
            placeholder = "@model.kontonummer",
            disabled = "disabled",
            @readonly = "readonly"
        }
    })
     </div>

        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.kontonavn)
            @Html.EditorFor(model => model.kontonavn, new {
            htmlAttributes = new {
                @class = "form-control",
                placeholder = "@model.kontonavn",
                disabled = "disabled",
                @readonly = "readonly"
            }
        })
        </div>

            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.saldo)
                @Html.EditorFor(model => model.saldo, new {
                htmlAttributes = new {
                    @class = "form-control",
                    placeholder = "@model.saldo",
                    disabled = "disabled",
                    @readonly = "readonly"
                }
            })
            </div>

            <div class="form-group col-md-12">
                <a href="@Url.Action("Transaksjoner", "Admin",
            new { knr = @Model.kontonummer })" class="btn btn-primary" title="Utførte betalinger"><span class="glyphicon glyphicon-info-sign"></span></a>
            </div>
}